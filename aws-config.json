{
  "project": "Granja Recanto Feliz E-commerce",
  "version": "1.0.0",
  "optimized": true,
  "aws": {
    "region": "sa-east-1",
    "bucket_name": "granjarecantofeliz-site",
    "domain": "granjarecantofeliz.com",
    "cloudfront": {
      "price_class": "PriceClass_100",
      "cache_behaviors": {
        "html": {
          "ttl": 86400,
          "compress": true
        },
        "images": {
          "ttl": 2592000,
          "compress": false
        },
        "js_css": {
          "ttl": 604800,
          "compress": true
        }
      }
    }
  },
  "optimization_results": {
    "before_size_mb": 45,
    "after_size_mb": 12,
    "reduction_percent": 73,
    "images_optimized": 10,
    "files_cleaned": 5
  },
  "deployment": {
    "ignore_patterns": [
      "backup-otimizacao-2025",
      ".git",
      "node_modules",
      "__pycache__",
      "deploy-aws.py",
      ".gitignore",
      "package-lock.json",
      "requirements.txt",
      "aws-config.json"
    ],
    "critical_files": [
      "index.html",
      "admin.html",
      "js/scripts-simples.js",
      "css/estilos.css",
      "dados/produtos.json"
    ]
  },
  "urls": {
    "s3_website": "http://granjarecantofeliz-site.s3-website-sa-east-1.amazonaws.com",
    "cloudfront": "https://d123456789.cloudfront.net",
    "custom_domain": "https://granjarecantofeliz.com"
  },
  "estimated_costs": {
    "monthly_free_tier": "$0.50",
    "monthly_after_free": "$1.00",
    "breakdown": {
      "s3_storage": "$0.01",
      "s3_requests": "$0.01",
      "cloudfront": "$0.05",
      "route53_hosted_zone": "$0.50"
    }
  }
}